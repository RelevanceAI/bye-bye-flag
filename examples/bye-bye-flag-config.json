{
  "fetcher": {
    "type": "posthog",
    "projectIds": [12345],
    "staleDays": 30
  },
  "agent": {
    "type": "claude",
    "timeoutMinutes": 60
  },
  "orchestrator": {
    "concurrency": 3,
    "maxPrs": 10,
    "logDir": "./bye-bye-flag-logs"
  },
  "repoDefaults": {
    "shellInit": "source ~/.nvm/nvm.sh && nvm use",
    "baseBranch": "main",
    "setup": ["pnpm install"]
  },
  "repos": {
    "my-repo": {
      "baseBranch": "main"
    }
  }
}
